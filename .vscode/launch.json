    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch JdbcSinkConfig",
            "request": "launch",
            "mainClass": "io.confluent.connect.jdbc.sink.JdbcSinkConfig",
            "projectName": "kafka-connect-jdbc"
        },
        {
            "type": "java",
            "name": "Attach JdbcSinkConfig",
            "request": "attach",
            "hostName": "10.10.21.164",
            "port": 5005,
            "timeout": 30000
        },
        {
            "type": "java",
            "name": "Launch JdbcSourceConnectorConfig",
            "request": "launch",
            "mainClass": "io.confluent.connect.jdbc.source.JdbcSourceConnectorConfig",
            "projectName": "kafka-connect-jdbc"
        }
    ]
}